! xterm settings
xterm*Background: black
xterm*Foreground: white
xterm*scrollBar: off
xterm*fadeColor: black
xterm*transparent: false
xterm*shading: 80
xterm*depth:24
xterm*borderless: 1
xterm*termName: xterm
xterm*title: xterm
xterm*urlLauncher: firefox
xterm*underlineURLs: true

! xterm black
xterm*color0:   #000000
xterm*color8:   #5C585C

! xterm blue
xterm*color4:   #4756F5
xterm*color12:  #002BFF

! xterm cyan
xterm*color6:   #6CF7EB
xterm*color14:  #07B099

! xterm green
xterm*color2:   #6eed44
xterm*color10:  #00FF11

! xterm magenta
xterm*color5:   #EE69FA
xterm*color13:  #FF00DD

! xterm red
xterm*color1:   #F54E00
xterm*color9:   #FF0000

! xterm yellow
xterm*color3:   #C4DE50
xterm*color11:  #DDFF00
 
! xterm white
xterm*color7:   #D6D6D6
xterm*color15:  #FFFFFF

! URxvt settings
URxvt.geometry: 247x68 
URxvt.scrollBar: off
URxvt.foreground: White
URxvt.background: Black
URxvt.fadeColor: black
URxvt.transparent: true 
URxvt.shading: 80
URxvt.depth:32
URxvt.borderless: 1
URxvt.termName: rxvt-256color
URxvt.title: urxvt
URxvt.urlLauncher: firefox
URxvt.underlineURLs: true
URxvt.perl-ext-common: ...,tabbed,...
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0 
URxvt.tabbed.tab-fg:    1
URxvt.tabbed.tab-bg:    0

! URxvt black
URxvt.color0:   #000000
URxvt.color8:   #5C585C

! URxvt blue
URxvt.color4:   #4756F5
URxvt.color12:  #002BFF

! URxvt cyan
URxvt.color6:   #6CF7EB
URxvt.color14:  #07B099

! URxvt green
URxvt.color2:   #6eed44
URxvt.color10:  #00FF11

! URxvt magenta
URxvt.color5:   #EE69FA
URxvt.color13:  #FF00DD

! URxvt red
URxvt.color1:   #F54E00
URxvt.color9:   #FF0000

! URxvt yellow
URxvt.color3:   #C4DE50
URxvt.color11:  #DDFF00

! URxvt white
URxvt.color7:   #D6D6D6
URxvt.color15:  #FFFFFF

! Xscreensaver settings
xscreensaver.Dialog.foreground: #1c4582
xscreensaver.Dialog.background: #000000
xscreensaver.Dialog.topShadowColor: #1c4582
xscreensaver.Dialog.bottomShadowColor: #1c4582
xscreensaver.Dialog.Button.foreground: #1c4582
xscreensaver.Dialog.Button.background: #000000
xscreensaver.Dialog.text.foreground: #1c4582
xscreensaver.Dialog.text.background: #000000
xscreensaver.Dialog.internalBorderWidth:24
xscreensaver.Dialog.borderWidth: 0
xscreensaver.Dialog.shadowThickness: 2
xscreensaver.passwd.thermometer.foreground: #ff0000
xscreensaver.passwd.thermometer.background: #000000
xscreensaver.passwd.thermometer.width: 8
xscreensaver.dateFormat: %I:%M%P %a %b %d, %Y

