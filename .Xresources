! Xpdf
Xpdf*enableFreetype: yes
Xpdf*antialias: yes
Xpdf*urlCommand: /usr/bin/firefox %s

! Xft settings
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

